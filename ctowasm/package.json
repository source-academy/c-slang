{
  "name": "ctowasm",
  "version": "1.0.6",
  "description": "Browser-based Typescript compiler from C to Webassembly",
  "repository": "git@github.com:source-academy/c-slang.git",
  "author": "benmurphyy <47269122+benmurphyy@users.noreply.github.com>",
  "license": "MIT",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-modules-commonjs": "^7.22.15",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.6.4",
    "@parcel/config-default": "^2.10.1",
    "@parcel/packager-ts": "2.10.1",
    "@parcel/transformer-inline-string": "2.10.1",
    "@parcel/transformer-js": "^2.10.1",
    "@parcel/transformer-react-refresh-wrap": "^2.10.1",
    "@parcel/transformer-typescript-types": "2.10.1",
    "@types/emscripten": "^1.39.10",
    "@types/jest": "^29.5.12",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "parcel": "^2.10.1",
    "peggy": "^4.0.2",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "ts-pegjs": "^4.2.1",
    "typescript": "^5.4.2",
    "yargs": "^17.7.2"
  },
  "scripts": {
    "build": "yarn clean-dist && yarn gen-parsers && parcel build --no-cache",
    "lint": "eslint . --fix",
    "prettify": "prettier . --write",
    "compile": "yarn build && node cmd.js compile",
    "compile-run": "yarn build && node cmd.js compile-run",
    "compile-wat": "yarn build && node cmd.js compile-to-wat",
    "gen-c-ast": "yarn build && node cmd.js generate-c-ast",
    "gen-p-c-ast": "yarn build && node cmd.js generate-processed-c-ast",
    "gen-wat-ast": "yarn build && node cmd.js generate-wat-ast",
    "test": "yarn build && NODE_OPTIONS=--experimental-vm-modules jest",
    "unit-test": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=unit_tests",
    "check": "tsc --noEmit",
    "clean-dist": "rimraf dist",
    "gen-preprocessor": "peggy ./src/parser/peggyjs/preprocessor.pegjs --cache --allowed-start-rules program",
    "gen-lexer": "peggy ./src/parser/peggyjs/lexer.pegjs --cache --allowed-start-rules program",
    "gen-parser": "peggy ./src/parser/peggyjs/parser.pegjs --cache --allowed-start-rules program",
    "gen-parsers": "yarn gen-preprocessor && yarn gen-lexer && yarn gen-parser"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "wabt": "^1.0.32"
  }
}
